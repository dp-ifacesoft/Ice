{
  "name": "ifacesoft/ice",
  "description": "Ice Open Source PHP Framework",
  "homepage": "http://iceframework.net",
  "type": "library",
  "keywords": ["ice", "framework", "orm"],
  "minimum-stability": "dev",
  "license": "BSD ICE",
  "authors": [
    {
      "name": "dp",
      "email": "denis.a.shestakov@gmail.com"
    }
  ],
  "config": {
    "vendor-dir": "../_vendor"
  },
  "autoload": {
    "psr-4": {"Ice\\": "Source/Ice/"}
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "jquery/jquery-ui",
        "version": "1.11.1",
        "dist": {
          "url": "http://jqueryui.com/resources/download/jquery-ui-1.11.1.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "smarty/smarty": "3.1.19",
    "twig/twig": "1.16.1",
    "twbs/bootstrap": "3.3.1",
    "jquery/jquery-ui": "1.11.1",
    "mrclay/minify": "dev-master",
    "facebook/xhprof": "dev-master"
  },
  "require-dev": {
    "firephp/firephp-core": "dev-master",
    "phpunit/phpunit": "4.4.0",
    "phpunit/php-invoker": "1.1.3",
    "phpunit/dbunit": "1.3.1",
    "phpunit/phpunit-selenium": "1.4.2",
    "phpunit/phpunit-story": "1.0.2",
    "ocramius/instantiator": "1.1.4"
  }
}
